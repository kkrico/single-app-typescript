{
  "name": "single-app-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run deploy && node dist/index.js",
    "start:dev": "concurrently \"npm:start:node\" \"npm:start:react\"",
    "start:react": "cd client && npm start",
    "start:node": "ts-node-dev --inspect=8181 --respawn --transpileOnly src/index.ts --watch client/dist/main.js",
    "lint": "concurrently \"npm:lint:node\" \"npm:lint:react\"",
    "lint:react": "cd client && eslint --fix ./src/*",
    "lint:node": "eslint --fix ./src/*",
    "test": "concurrently \"npm:test:node\" \"npm:test:react\"",
    "test:node": "jest --passWithNoTests",
    "test:react": "cd client && jest --passWithNoTests",
    "install:all": "yarn install && cd client && yarn install",
    "deploy": "tsc --project tsconfig.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.7.5",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.2.2",
    "prettier": "^2.0.2",
    "ts-jest": "^25.2.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
